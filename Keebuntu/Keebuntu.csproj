<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
    <Platform Condition=" '$(Platform)' == '' ">AnyCPU</Platform>
    <ProductVersion>10.0.0</ProductVersion>
    <SchemaVersion>2.0</SchemaVersion>
    <ProjectGuid>{AF47CF84-8823-4A58-9E79-543970D5BCA1}</ProjectGuid>
    <OutputType>Library</OutputType>
    <RootNamespace>Keebuntu</RootNamespace>
    <AssemblyName>Keebuntu</AssemblyName>
    <Description>Plugin for KeePass that add Ubuntu Application Indicator support</Description>
    <ReleaseVersion>0.1.0</ReleaseVersion>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' ">
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
    <Optimize>false</Optimize>
    <OutputPath>bin\Debug</OutputPath>
    <DefineConstants>DEBUG;</DefineConstants>
    <ErrorReport>prompt</ErrorReport>
    <WarningLevel>4</WarningLevel>
    <ConsolePause>false</ConsolePause>
    <CustomCommands>
      <CustomCommands>
        <Command type="Execute" command="bin/Debug/KeePass.exe --debug" />
      </CustomCommands>
    </CustomCommands>
    <EnvironmentVariables>
      <EnvironmentVariables>
        <Variable name="MONO_TRACE_LISTENER" value="Console.Error" />
        <Variable name="UBUNTU_MENUPROXY" value="libappmenu.so" />
      </EnvironmentVariables>
    </EnvironmentVariables>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <DebugType>none</DebugType>
    <Optimize>false</Optimize>
    <OutputPath>bin\Release</OutputPath>
    <WarningLevel>4</WarningLevel>
    <CustomCommands>
      <CustomCommands>
        <Command type="Execute" command="bin/Debug/KeePass.exe" />
      </CustomCommands>
    </CustomCommands>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'ReleasePlgx|AnyCPU' ">
    <DebugType>none</DebugType>
    <Optimize>false</Optimize>
    <OutputPath>bin\ReleasePlgx</OutputPath>
    <WarningLevel>4</WarningLevel>
    <CustomCommands>
      <CustomCommands>
        <Command type="Execute" command="bin/ReleasePlgx/KeePass.exe --debug --saveplgxcr" />
      </CustomCommands>
    </CustomCommands>
  </PropertyGroup>
  <PropertyGroup>
    <PlgxConfiguration>
      <Prerequisites>
        <OS>Unix</OS>
      </Prerequisites>
      <BuildCommands>
        <PreBuild>rm {PLGX_TEMP_DIR}Keebuntu.csproj</PreBuild>
      </BuildCommands>
    </PlgxConfiguration>
  </PropertyGroup>
  <ItemGroup>
    <Reference Include="gtk-sharp">
      <Package>gtk-sharp-2.0</Package>
      <Private>False</Private>
    </Reference>
    <Reference Include="appindicator-sharp">
      <Package>appindicator-sharp-0.1</Package>
      <Private>False</Private>
    </Reference>
    <Reference Include="System" />
    <Reference Include="System.Windows.Forms" />
    <Reference Include="System.Drawing" />
    <Reference Include="dbus-sharp">
      <Package>dbus-sharp-1.0</Package>
      <Private>False</Private>
    </Reference>
    <Reference Include="dbus-sharp-glib">
      <Package>dbus-sharp-glib-1.0</Package>
      <Private>False</Private>
    </Reference>
    <Reference Include="gdk-sharp">
      <Package>gtk-sharp-2.0</Package>
      <Private>False</Private>
    </Reference>
    <Reference Include="KeePass.exe">
      <HintPath>$(KeePassPath)KeePass.exe</HintPath>
    </Reference>
  </ItemGroup>
  <Import Project="$(MSBuildBinPath)\Microsoft.CSharp.targets" />
  <Import Project="$(KeePassPluginDevToolsPath)\Plgx.targets" />
  <ProjectExtensions>
    <MonoDevelop>
      <Properties>
        <Deployment.LinuxDeployData generateScript="false" />
      </Properties>
    </MonoDevelop>
  </ProjectExtensions>
  <ItemGroup>
    <Compile Include="AssemblyInfo.cs" />
    <Compile Include="KeebuntuExt.cs" />
    <Compile Include="Registrar.cs" />
    <Compile Include="IDBusMenu.cs" />
    <Compile Include="IService.cs" />
    <Compile Include="IMenuItemProxy.cs" />
    <Compile Include="DefaultMenuItemProxy.cs" />
    <Compile Include="ToolStripItemProxy.cs" />
    <Compile Include="MenuStripProxy.cs" />
    <Compile Include="MenuStripDBusMenu.cs" />
  </ItemGroup>
  <ItemGroup>
    <None Include="KeePass.config.xml">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <ExcludeFromPlgx />
    </None>
    <None Include="KeePass.exe.config">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <ExcludeFromPlgx />
    </None>
    <None Include="Resources\icons\ubuntu-mono-dark\apps\24\keepass2.svg">
      <ExcludeFromPlgx />
    </None>
    <None Include="Resources\icons\ubuntu-mono-light\apps\24\keepass2.svg">
      <ExcludeFromPlgx />
    </None>
  </ItemGroup>
  <Target Name="AfterBuild" Condition="'$(Configuration)' == 'ReleasePlgx'">
    <CallTarget Targets="BuildPlgx" />
    <Delete Files="$(OutDir)$(AssemblyName)$(TargetExt)" />
  </Target>
</Project>
